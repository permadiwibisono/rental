{
  "name": "rental",
  "version": "1.0.0",
  "description": "Rental",
  "main": "dist/server.js",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf dist/",
    "start": "npm run serve",
    "serve": "node dist/server",
    "dev": "concurrently -k -p \"[{name}]\" -n -c \"cyan.bold,green.bold\" \"npm run watch:*\"",
    "watch:node": "nodemon dist/server",
    "watch:ts": "tsc -w",
    "watch:ts:alias": "tsc-alias -w",
    "build": "tsc && tsc-alias",
    "tsc:check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/permadiwibisono/rental.git"
  },
  "keywords": [],
  "author": "Permadi Wibisono",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/permadiwibisono/rental/issues"
  },
  "homepage": "https://github.com/permadiwibisono/rental#readme",
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^12.0.3",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "http-errors": "^2.0.0",
    "mongoose": "^6.1.6",
    "morgan": "^1.10.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.5.4"
  }
}
