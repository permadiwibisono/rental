{
  "name": "rental",
  "version": "1.0.0",
  "description": "Rental",
  "main": "dist/server.js",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf dist/",
    "start": "npm run serve",
    "serve": "node dist/server",
    "dev": "concurrently -k -p \"[{name}]\" -n -c \"cyan.bold,green.bold\" \"npm run watch:*\"",
    "watch:node": "nodemon dist/server",
    "watch:ts": "tsc -w",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/permadiwibisono/rental.git"
  },
  "keywords": [],
  "author": "Permadi Wibisono",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/permadiwibisono/rental/issues"
  },
  "homepage": "https://github.com/permadiwibisono/rental#readme",
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "express": "^4.17.2"
  }
}
